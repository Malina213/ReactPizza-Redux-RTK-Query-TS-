{
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/ui/",
		"/hooks/",
		"/components/",
		"^../(.*)",
		"^./(.*)",
		"*.css"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid"
}
